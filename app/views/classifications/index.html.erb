<h1>All Classifiers</h1>
<% @classifiers.each do |x| %>
  <p>ID is: <%= x["classifier_id"]%></p>
  <p>URL is: <%= x["url"]%></p>
  <p>Name is: <%= x["name"]%></p>
  <p>Created is: <%= x["created"]%></p>
  
  
  <button type="button"><%= link_to "SHOW", classifiers_show_path(:id => x["classifier_id"]) %></button> 

  <div>
<button type="button" class="btn btn-outline-primary"><%= link_to "HOME", root_path%></button>
</div> 

   
<% end %> 